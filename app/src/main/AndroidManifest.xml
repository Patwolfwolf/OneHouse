        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".secondLevel"
            android:label="@string/title_activity_second_level"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".seconLevelPhysics"
            android:label="@string/title_activity_secon_level_physics"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".thridLevelGeometry"
            android:label="@string/title_activity_thrid_level_geometry"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".rectangleDef"
            android:label="@string/title_activity_rectangle_def"
